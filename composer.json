{
    "name": "andygrunwald/Jacobine",
    "description": "Base package for a big and awesome analysis suite to crawl and analyse various data of software development.",
    "keywords": ["analysis", "software repository mining", "data mining", "messaging", "knowledge"],
    "homepage": "https://github.com/andygrunwald/Jacobine",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Grunwald",
            "email": "andygrunwald@gmail.com",
            "homepage": "http://andygrunwald.com/"
        }
    ],
    "support": {
        "email": "andygrunwald@gmail.com",
        "issues": "https://github.com/andygrunwald/Jacobine/issues",
        "source": "https://github.com/andygrunwald/Jacobine.git"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/andygrunwald/Gerrie"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-curl": "*",
        "videlalvaro/php-amqplib": "2.4.*",
        "kriswallsmith/buzz": "0.*",
        "symfony/yaml": "2.6.*",
        "symfony/console": "2.6.*",
        "symfony/dom-crawler": "2.6.*",
        "symfony/process": "2.8.*",
        "symfony/dependency-injection":"2.6.*",
        "symfony/config":"2.6.*",
        "phploc/phploc": "2.0.*",
        "pdepend/pdepend": "2.0.*",
        "monolog/monolog": "1.26.*",
        "andygrunwald/gerrie": "0.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.*",
        "phpunit/dbunit": "1.3.*",
        "mikey179/vfsStream": "1.4.*"
    },
    "autoload": {
        "psr-0": {"": "src/"}
    },
    "config": {
      "bin-dir": "bin"
    }
}
